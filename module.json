{
  "id": "drag-ruler",
  "title": "Drag Ruler v13+",
  "description": "When dragging a token displays a ruler showing how far you've moved that token.",
  "version": "2.0.0",
  "compatibility": {
    "minimum": "13",
    "verified": "13"
  },
  "authors": [
    {
      "name": "Manuel Vögele",
      "email": "develop@manuel-voegele.de",
      "discord": "stabchenfisch"
    }
  ],
  "esmodules": [
    "src/libwrapper_shim.js",
    "src/main.js",
    "src/socket.js"
  ],
  "templates": [
    "templates/speed_provider_settings.html"
  ],
  "languages": [
    {
      "lang": "de",
      "name": "Deutsch",
      "path": "lang/de.json"
    },
    {
      "lang": "en",
      "name": "English",
      "path": "lang/en.json"
    },
    {
      "lang": "es",
      "name": "Español",
      "path": "lang/es.json"
    },
    {
      "lang": "fr",
      "name": "Français",
      "path": "lang/fr.json"
    },
    {
      "lang": "ja",
      "name": "日本語",
      "path": "lang/ja.json"
    },
    {
      "lang": "ko",
      "name": "한국어",
      "path": "lang/ko.json"
    },
    {
      "lang": "zh-tw",
      "name": "正體中文",
      "path": "lang/zh-tw.json"
    },
    {
      "lang": "cn",
      "name": "中文（简体）",
      "path": "lang/zh_Hans.json"
    },
    {
      "lang": "pt-BR",
      "name": "Português (Brasil)",
      "path": "lang/pt_BR.json"
    }
  ],
  "relationships": {
    "requires": [
      {
        "id": "socketlib",
        "type": "module",
        "manifest": "https://raw.githubusercontent.com/ottoloot/foundryvtt-socketlib/master/module.json"
      }
    ]
  },
  "socket": true,
  "url": "https://github.com/ottoloot/foundryvtt-drag-ruler-v13",
  "download": "https://github.com/ottoloot/foundryvtt-drag-ruler-v13/archive/refs/tags/v2.0.0.zip",
  "manifest": "https://raw.githubusercontent.com/ottoloot/foundryvtt-drag-ruler-v13/develop/module.json",
  "readme": "https://github.com/ottoloot/foundryvtt-drag-ruler-v13/blob/develop/README.md",
  "changelog": "https://github.com/ottoloot/foundryvtt-drag-ruler-v13/blob/develop/CHANGELOG.md",
  "bugs": "https://github.com/ottoloot/foundryvtt-drag-ruler-v13/issues",
  "allowBugReporter": true
}
